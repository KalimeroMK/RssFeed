{
    "name": "kalimeromk/rssfeed",
    "description": "A simple RSS feed reader for Laravel",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Zorab Shefot Bogoevski",
            "email": "zbogoevski@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "Kalimeromk\\Rssfeed\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kalimeromk\\Rssfeed\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kalimeromk\\Rssfeed\\RssfeedServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "simplepie/simplepie": "^1.8.0",
        "ext-curl": "*",
      "ext-libxml": "*",
      "ext-dom": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^9",
        "phpunit/phpunit": "^11.5.6",
        "rector/rector": "2.0.9"
    },
    "minimum-stability": "dev"
}
